@model ProductEmptyViewModel

<div class="mt-3 mb-5">
    <div class="container-fluid text-center">
        @if (Model.CategoryName != "Other")
        {
            <h4>We are sorry, products of @Model.CategoryName.ToLower() are sold.</h4>
        }
        else
        {
            <h4>We are sorry, all other products are sold.</h4>
        }

    </div>
    <hr class="hr-2 bg-dark" />
</div>